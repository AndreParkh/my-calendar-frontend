{
  "name": "my-calendar-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_API_DOMAIN=http://localhost:3000/api node server",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,html}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:css": "stylelint \"**/*.{css,scss,sass,less}\"",
    "lint:css:fix": "stylelint \"**/*.{css,scss,sass,less}\" --fix",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry.server.tsx --outDir dist/server",
    "start": "cross-env NODE_ENV=production node server",
    "prepare": "husky"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.11.0",
    "compression": "^1.8.0",
    "express": "^5.1.0",
    "i18next": "^25.3.2",
    "i18next-http-backend": "^3.0.2",
    "js-cookie": "^3.0.5",
    "micromatch": "^4.0.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-i18next": "^15.6.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.3",
    "serialize-javascript": "^6.0.2",
    "sirv": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@types/express": "^5.0.3",
    "@types/js-cookie": "^3.0.6",
    "@types/micromatch": "^4.0.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "dotenv-expand": "^12.0.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "reset-css": "^5.0.2",
    "stylelint": "^16.23.0",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
